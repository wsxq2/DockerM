<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import org.controlsfx.control.CheckComboBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" prefHeight="715.0" prefWidth="681.0" spacing="10.0"
      xmlns="http://javafx.com/javafx/11.0.1">
    <children>
        <GridPane vgap="5.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Label text="Title"/>
                <Label text="Description" GridPane.rowIndex="1"/>
                <TextField GridPane.columnIndex="1"/>
                <TextField GridPane.columnIndex="1" GridPane.rowIndex="1"/>
            </children>
        </GridPane>
        <Separator prefWidth="200.0"/>
        <GridPane vgap="5.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="-Infinity" prefHeight="80.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Label text="Name"/>
                <Label text="Logo URL" GridPane.rowIndex="1"/>
                <Label text="Note" GridPane.rowIndex="2"/>
                <TextField GridPane.columnIndex="1"/>
                <TextField GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                <TextArea prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
                <Label text="Platform" GridPane.rowIndex="3"/>
                <Label text="Categories" GridPane.rowIndex="4"/>
                <ChoiceBox prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
                <CheckComboBox GridPane.columnIndex="1" GridPane.rowIndex="4"/>
            </children>
        </GridPane>
        <Separator prefWidth="200.0"/>
        <TextArea prefHeight="200.0" prefWidth="200.0" promptText="docker compose file"/>
        <Separator prefWidth="200.0"/>
        <GridPane vgap="5.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints/>
            </rowConstraints>
            <children>
                <fx:include source="AccessControl.fxml" GridPane.columnIndex="1"/>
                <Label text="Access control"/>
            </children>
        </GridPane>
        <Separator prefWidth="200.0"/>
        <Button mnemonicParsing="false" text="Create"/>
    </children>
    <opaqueInsets>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </opaqueInsets>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
</VBox>
